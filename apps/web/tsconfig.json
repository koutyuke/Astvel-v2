{
  "extends": "@astvel/tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "#core/*": ["../../packages/core/*"],
      "#styles/*": ["styled-system/*"],
    },
    "strictNullChecks": true
  },
  "include": ["**/*.ts", "**/*.tsx", "next-env.d.ts", ".next/types/**/*.ts"],
  "exclude": ["**/node_modules/**/*"]
}
